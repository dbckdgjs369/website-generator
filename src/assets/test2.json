{
  "div": [
    {
      "header": [
        {
          "style": {
            "position": "relative",
            "display": "flex",
            "alignItems": "center",
            "justifyContent": "space-between",
            "padding": "0 32px",
            "height": "56px",
            "backgroundColor": "#fff",
            "boxShadow": "0 2px 4px 0 rgba(0, 0, 0, 0.1)"
